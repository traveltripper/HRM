<p id="notice"><%= notice %></p>

<h1>Listing Healthinsurances</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Card number</th>
      <th>Relation</th>
      <th>Issued date</th>
      <th>Policy start date</th>
      <th>Policy end date</th>
      <th>Employee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @healthinsurances.each do |healthinsurance| %>
      <tr>
        <td><%= healthinsurance.name %></td>
        <td><%= healthinsurance.card_number %></td>
        <td><%= healthinsurance.relation %></td>
        <td><%= healthinsurance.issued_date %></td>
        <td><%= healthinsurance.policy_start_date %></td>
        <td><%= healthinsurance.policy_end_date %></td>
        <td><%= healthinsurance.employee_id %></td>
        <td><%= link_to 'Show', healthinsurance %></td>
        <td><%= link_to 'Edit', edit_healthinsurance_path(healthinsurance) %></td>
        <td><%= link_to 'Destroy', healthinsurance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Healthinsurance', new_healthinsurance_path %>
