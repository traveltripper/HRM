<h4>Upcoming Company Events</h4>
<div class="row">
<% upcoming_company_events.each do |f| %>
	<div class="col-md-6">	
		<h5 class="event-title-color"><%=f.title%></h5>
		<p><%=f.start.strftime("%A, %B %d")%></p>            
		<%=image_tag f.picture.url(:medium)%>
		<p><%=f.reason.html_safe%></p>	
	</div>
<%end%>
</div>